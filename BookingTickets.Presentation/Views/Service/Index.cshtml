@using BookingTickets.Core.Entities
@model List<Service>
<!--Page Header Start-->
<section class="page-header">
    <div class="page-header__bg" style="background-image: url(https://unicktheme.com/2024/eventflow/main-html/assets/images/backgrounds/page-header-bg.jpg);">
    </div>
    <div class="page-header__shape-1 float-bob-y">
        <img src="https://unicktheme.com/2024/eventflow/main-html/assets/images/backgrounds/page-header-bg.jpg" alt="">
    </div>
    <div class="page-header__shape-2 float-bob-x">
        <img src="https://unicktheme.com/2024/eventflow/main-html/assets/images/backgrounds/page-header-bg.jpg" alt="">
    </div>
    <div class="container">
        <div class="page-header__inner">
            <h2>Services</h2>
            <div class="thm-breadcrumb__box">
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><span class="icon-angle-right"></span></li>
                    <li>Services</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<!--Page Header End-->
<!--Services Page Start-->
<section class="services-page">
    <div class="container">
        <div class="row">
            @await Component.InvokeAsync("Services")
        </div>
    </div>
</section>
<!--Services Page End-->